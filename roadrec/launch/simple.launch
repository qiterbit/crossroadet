<launch>
	<node pkg="hokuyo_node" type="hokuyo_node" name="hokuyo_node" />
	<node pkg="roadrec" type="gridmap" name="gridmap" output="screen" />

	<!-- <node pkg="roadrec" type="testScan" name="test" output="screen" /> -->
	<node pkg="tf" type="static_transform_publisher" name="base_link_to_laserfake_broadcaster" args="0 0 0.2 0 0 0 1 /base_link /laserfake 100" />
	<node pkg="tf" type="static_transform_publisher" name="base_link_to_laser_broadcaster" args="0 0 0.2 0 0 0 1 /base_link /laser 100" /> 
	<node pkg="tf" type="static_transform_publisher" name="base_footprint_to_base_link_broadcaster" args="0 0 0 0 0 0 1 /base_footprint /base_link 100" />  
	<node pkg="tf" type="static_transform_publisher" name="odom_to_base_footprint_broadcaster" args="0 0 0 0 0 0 1 /odom /base_footprint 100" />  
	<node pkg="tf" type="static_transform_publisher" name="map_to_odom_broadcaster" args="0 0 0 0 0 0 1 /map /odom 100" />  

	<node name="modelvisualisation" pkg="rviz" type="rviz" output="screen" /> 
</launch>
